{
  "workbench.startupEditor": "none",
  "editor.fontLigatures": true,
  "editor.fontFamily": "FiraCode Nerd Font",
  "editor.fontWeight": "bold",
  "editor.fontSize": 16,
  "workbench.iconTheme": "material-icon-theme",
  "editor.formatOnSave": true,
  "[python]": {
    "editor.defaultFormatter": "ms-python.black-formatter"
  },
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "workbench.editor.enablePreview": false,
  "github.copilot.advanced": {},
  "workbench.colorTheme": "Material Theme Darker High Contrast",
  "workbench.colorCustomizations": {
    "[Material Theme Darker High Contrast]": {
      "sideBar.background": "#1a1a1a",
      "sideBarSectionHeader.background": "#1a1a1a",
      "editor.background": "#1a1a1a",
      "activityBar.background": "#1a1a1a",
      "statusBar.background": "#1a1a1a",
      "panel.background": "#1a1a1a",
      "titleBar.activeBackground": "#1a1a1a",
      "tab.activeBackground": "#1a1a1a",
      "tab.inactiveBackground": "#1a1a1a",
      "editorGroupHeader.tabsBackground": "#1a1a1a",
      "editorGroupHeader.noTabsBackground": "#1a1a1a",
      "breadcrumb.background": "#1a1a1a",
      "tab.activeBorder": "#2979ff",
      "gitDecoration.modifiedResourceForeground": "#2979ff",
      "gitDecoration.deletedResourceForeground": "#2979ff",
      "gitDecoration.untrackedResourceForeground": "#2979ff",
      "statusBarItem.remoteBackground": "#2979ff",
      "activityBarBadge.background": "#2979ff"
    }
  },
  "editor.tokenColorCustomizations": {
    "[Material Theme Darker High Contrast]": {
      "textMateRules": [
        {
          "scope": "entity.other.attribute-name.id.css",
          "settings": {
            "foreground": "#c18ee3"
          }
        }
      ]
    }
  }
}
